{
  "name": "zentik-mobile",
  "main": "expo-router/entry",
  "version": "0.0.97",
  "scripts": {
    "start": "expo start",
    "export:web": "npx expo export --platform web --output-dir dist",
    "build:web": "npx expo export -p web --output-dir dist && npx workbox-cli generateSW workbox-config.js",
    "deploy:patch": "node scripts/deploy-pwa.js patch",
    "deploy:minor": "node scripts/deploy-pwa.js minor",
    "deploy:major": "node scripts/deploy-pwa.js major",
    "lint": "npx expo lint",
    "codegen": "graphql-codegen",
    "release:ios:patch": "node scripts/release-ios.js patch",
    "release:ios:minor": "node scripts/release-ios.js minor",
    "release:ios:major": "node scripts/release-ios.js major",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "submit:all": "eas submit --platform all --latest",
    "update": "eas update --environment production --channel production",
    "update:patch": "node scripts/update.js patch",
    "update:minor": "node scripts/update.js minor",
    "update:major": "node scripts/update.js major",
    "start:ios": "npx expo run:ios --device",
    "start:dev": "npx expo start --dev-client",
    "start:prod": "npx expo start --no-dev --minify",
    "start:android": "npx expo run:android --device",
    "start:web": "npx expo start --web",
    "prebuild:ios": "npx expo prebuild --clean --platform ios",
    "prebuild:android": "npx expo prebuild --clean --platform android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "regen:icons": "node scripts/regen-icons.js",
    "eas-build-pre-install": "npm config set legacy-peer-deps true"
  },
