{
  "name": "zentik-mobile",
  "main": "index.js",
  "version": "0.1.207",
  "dockerVersion": "0.0.153",
  "scripts": {
    "start": "expo start",
    "eas-build-post-install": "node scripts/post-build-ios.js",
    "export:web": "npx expo export --platform web --output-dir dist",
    "build:web": "npx expo export -p web --output-dir dist && npx workbox-cli injectManifest workbox-config.js",
    "release:web:patch": "node scripts/release-web.js patch",
    "release:web:minor": "node scripts/release-web.js minor",
    "release:web:major": "node scripts/release-web.js major",
    "lint": "npx expo lint",
    "ts-check": "tsc --noEmit",
    "codegen": "graphql-codegen",
    "sync:privacy-policy": "node scripts/sync-privacy-policy.js",
    "release:ios:patch": "node scripts/release-ios.js patch",
    "release:ios:minor": "node scripts/release-ios.js minor",
    "release:ios:major": "node scripts/release-ios.js major",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "submit:all": "eas submit --platform all --latest",
    "update": "eas update --environment production --channel production",
    "update:patch": "node scripts/update.js patch",
    "update:minor": "node scripts/update.js minor",
    "update:major": "node scripts/update.js major",
    "start:ios": "npx expo run:ios --device",
    "start:dev": "npx expo start --dev-client",
    "start:prod": "npx expo start --no-dev --minify",
    "start:android": "npx expo run:android --device",
    "start:web": "npx expo start --web",
    "prebuild:ios": "npx expo prebuild --clean --platform ios && node scripts/post-build-ios.js",
    "prebuild:android": "npx expo prebuild --clean --platform android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "regen:icons": "node scripts/regen-icons.js",
    "generate:i18n-types": "node scripts/generate-i18n-types.js",
    "sync-ios-extensions": "bash scripts/sync-ios-extensions.sh",
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@apollo/client": "^3.14.0",
    "@bacons/apple-targets": "^3.0.5",
    "@expo/metro-runtime": "~6.1.2",
    "@material/material-color-utilities": "^0.3.0",
    "@react-native-community/hooks": "^3.0.0",
    "@rivascva/react-native-code-editor": "^1.2.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-firebase/app": "^23.0.1",
    "@react-native-firebase/messaging": "^23.0.1",
    "@react-native-vector-icons/material-design-icons": "^12.3.0",
    "@react-navigation/core": "^7.12.4",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.8",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^5.90.2",
    "expo": "~54.0.31",
    "expo-apple-authentication": "~8.0.8",
    "expo-asset": "~12.0.9",
    "expo-audio": "~1.1.1",
    "expo-background-task": "~1.0.10",
    "expo-build-properties": "~1.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.8",
    "expo-device": "~8.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.14",
    "expo-font": "~14.0.8",
    "expo-image": "~3.0.11",
    "expo-image-manipulator": "~14.0.8",
    "expo-linking": "~8.0.11",
    "expo-localization": "~17.0.8",
    "expo-media-library": "~18.2.1",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.21",
    "expo-screen-orientation": "~9.0.8",
    "expo-share-extension": "^5.0.0",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-task-manager": "~14.0.9",
    "expo-updates": "~29.0.16",
    "expo-video": "~3.0.15",
    "expo-video-thumbnails": "~10.0.8",
    "expo-web-browser": "~15.0.10",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.6",
    "handlebars": "^4.7.8",
    "idb": "^8.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keychain": "^10.0.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.14.5",
    "react-native-paper-dates": "^0.22.50",
    "react-native-popup-menu": "^0.18.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "reanimated-color-picker": "^4.1.0"
  },
  "devDependencies": {
    "sharp": "^0.34.3",
    "@0no-co/graphqlsp": "^1.15.0",
    "@babel/core": "^7.25.2",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-document-nodes": "^4.0.16",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@types/react": "~19.1.10",
    "dotenv": "^17.2.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "patch-package": "^8.0.0",
    "tsx": "^4.20.4",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "expo-share-extension",
          "@react-native-vector-icons/material-design-icons",
          "@bacons/apple-targets"
        ]
      }
    }
  },
  "private": true
}
